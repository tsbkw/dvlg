{
  "name": "dvlg",
  "version": "0.1.0",
  "description": "Blog framework to create blog site composed of static resources, generated by nuxtjs",
  "author": "tsbkw",
  "private": true,
  "scripts": {
    "dev": "nuxt --port 30080",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "jest",
    "dev-debug": "node --inspect-brk=9229 node_modules/nuxt/bin/nuxt --port 30080"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/markdownit": "^1.2.3",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "@types/cheerio": "^0.22.11",
    "@types/jsdom": "^12.2.3",
    "ava": "^2.1.0",
    "axios": "^0.19.0",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^5.2.0",
    "nuxt": "^2.4.0",
    "terser": "^4.0.0",
    "ts-node": "^8.1.0",
    "vue-property-decorator": "^8.1.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@nuxt/typescript": "^2.6.2",
    "@types/jest": "^24.0.13",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "jest": "^24.8.0",
    "nodemon": "^1.18.9",
    "ts-jest": "^24.0.2",
    "vue-jest": "^3.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      ".*\\.(js)$": "<rootDir>/node_modules/babel-jest"
    },
    "testURL": "http://localhost/"
  }
}
